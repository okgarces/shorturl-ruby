
<h1>Top 100 URLs</h1>
<table>
  <thead>
  <tr>
    <th>Title</th>
    <th>Original</th>
    <th> Short URL </th>
    <th> Count </th>
  </tr>
  </thead>
<tbody>
<% @urls.each do |url| %>
  <tr>
    <td> <%= url.title %> </td>
    <td> <%= url.original %> </td>
    <td> <%= url.short %> </td>
    <td> <%= url.count %> </td>
  </tr>
<% end %>
</tbody>
</table>
